{
  "name": "notes-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node dist/bin/www",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "test": "jest",
    "dev": "nodemon run start"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.1"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "dist/*"
    ]
  }
}
